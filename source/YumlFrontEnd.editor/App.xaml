<Application x:Class="YumlFrontEnd.editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:YumlFrontEnd.editor"
             xmlns:cal="http://www.caliburnproject.org"
             xmlns:converter="clr-namespace:YumlFrontEnd.editor.Converter">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Microsoft.Expression.Prototyping.SketchControls;component/ScrollViewerStyles.xaml"/>
        <ResourceDictionary Source="Styles/ColorStyles.xaml"/>
        <ResourceDictionary Source="SketchStyles.xaml" />
        <ResourceDictionary Source="Resources/Images.xaml"/>
        <ResourceDictionary Source="Styles/ButtonStyles.xaml"/>
        <ResourceDictionary Source="Styles/ControlStyles.xaml"/>
        <ResourceDictionary>
          <local:Bootstrapper x:Key="Bootstrapper" />
          <BooleanToVisibilityConverter x:Key="BooleanToVisibility"/>
          <converter:IsVisibleToColorConverter
            x:Key="IsVisibleToColor"
            VisibleBrush="{StaticResource BaseForeground-Sketch}"
            InvisibleBrush="{StaticResource DomainObjectIsInvisibleTextColor}"/>
        </ResourceDictionary>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
