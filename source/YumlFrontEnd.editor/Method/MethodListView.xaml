<UserControl x:Class="YumlFrontEnd.editor.MethodListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:caliburn="clr-namespace:Caliburn.Micro;assembly=Caliburn.Micro.Platform"
             xmlns:local="clr-namespace:YumlFrontEnd.editor"
             xmlns:yuml="clr-namespace:Yuml;assembly=YumlFrontEnd"
             d:DataContext="{d:DesignInstance local:MethodListViewModel}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
  <UserControl.Resources>
   
  </UserControl.Resources>
  <local:MemberListControl
    Title="{x:Static yuml:Strings.Methods}"
    MemberItemStyle="{StaticResource MemberItemsControl}"/>
</UserControl>
